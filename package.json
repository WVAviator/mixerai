{
  "name": "mixerai",
  "version": "0.0.1",
  "description": "An app that allows the use of AI to generate a completely unique cocktail recipe based on a simple prompt.",
  "main": "index.js",
  "author": "WVAviator",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:server": "cd packages/server && docker compose up",
    "start:mobile": "cd packages/mobile && yarn start",
    "test": "lerna run test",
    "lint": "lerna run lint",
    "typescript": "lerna run typescript",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.2"
  },
  "packageManager": "yarn@3.3.1"
}
